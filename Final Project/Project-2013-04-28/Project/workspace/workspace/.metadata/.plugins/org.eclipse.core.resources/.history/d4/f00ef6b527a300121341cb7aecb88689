<script type="text/javascript">
  function ValidateEmail()
  {
  var uemail = register.email;
  var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
  if(uemail.value.match(mailformat))
  {
  return true;
  }
  else
  {
  alert("You have entered an invalid email address!");
  uemail.value="";
  uemail.focus();

  return false;
  }
  }
   
  </script>
